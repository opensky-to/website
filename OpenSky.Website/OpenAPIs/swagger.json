          "password": {
            "type": "string",
            "description": "Gets or sets the password."
          },
          "recaptchaToken": {
            "type": "string",
            "description": "Gets or sets the recaptcha token.",
            "nullable": true
          },
          "username": {
            "type": "string",
            "description": "Gets or sets the username."
          }
        },
        "additionalProperties": false,
        "description": "Login model."
      },
      "LoginResponse": {
        "type": "object",
